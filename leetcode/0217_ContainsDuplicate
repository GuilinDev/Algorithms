class Solution {
    //下面这种排序后直接查的效率比较好，用HashSet检查duplicates的效率比较低
    //如果想找出哪些数字有重复，建一个ArrayList就行了
    public boolean containsDuplicate(int[] nums) {
        if (nums == null || nums.length == 0) {
            return false;
        }
        Arrays.sort(nums);
        for (int i = 1; i < nums.length; i++) {
            int temp = nums[i-1];
            if(temp == nums[i]) {
                return true;
            }
        }
        return false;
    }
}
